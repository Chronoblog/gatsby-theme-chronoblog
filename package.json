{
  "name": "gatsby-theme-chronoblog-workspace",
  "private": true,
  "license": "MIT",
  "author": "Ivan Ganev <ganevio92@gmail.com>",
  "workspaces": [
    "packages/*",
    "packages/@chronoblog/*",
    "starters/*",
    "examples/*",
    "test-builds/*"
  ],
  "scripts": {
    "clean:chronoblog": "cd starters/chronoblog && npm run clean",
    "fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "generate-starters": "node ./scripts/generate-starters",
    "lerna": "lerna",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "version:major": "node ./scripts/generate-starters && lerna version major --no-changelog",
    "version:minor": "node ./scripts/generate-starters && lerna version minor --no-changelog",
    "version:patch": "node ./scripts/generate-starters && lerna version patch --no-changelog",
    "version:premajor": "node ./scripts/generate-starters && lerna version premajor --no-changelog",
    "version:preminor": "node ./scripts/generate-starters && lerna version preminor --no-changelog",
    "version:prepatch": "node ./scripts/generate-starters && lerna version prepatch --no-changelog",
    "start:chronoblog": "cd starters/chronoblog && npm run clean && npm run start"
  },
  "devDependencies": {
    "@types/lodash": "4.14.168",
    "@types/node": "14.14.31",
    "@types/react": "16.14.4",
    "@types/react-dom": "16.9.11",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "babel-preset-gatsby": "0.12.1",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.0.0",
    "eslint-plugin-html": "6.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "fs-extra": "8.1.0",
    "fsevents": "2.3.2",
    "identity-obj-proxy": "3.0.0",
    "lerna": "4.0.0",
    "prettier": "2.2.1",
    "typescript": "4.1.5"
  },
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0",
    "yarn": "! use npm !"
  }
}
